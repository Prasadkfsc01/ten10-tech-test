{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "es6"],
    "types": ["cypress"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "baseUrl": "./cypress"
  },
  "include": ["cypress/**/*.ts"]
}
